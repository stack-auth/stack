      "source.fixAll.eslint": "explicit",
      "source.organizeImports": "explicit"
  },
  "terminal.integrated.wordSeparators": " (){}',\"`─‘’“”|",
  "editor.formatOnSave": false,
  "prettier.enable": false,
  "debug.javascript.autoAttachSmartPattern": [
    "${workspaceFolder}/**",
