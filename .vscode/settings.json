{
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.tabSize": 2,
  "cSpell.words": [
    "codegen",
    "Crudl",
    "deindent",
    "Falsey",
    "frontends",
    "hookform",
    "hostable",
    "INBUCKET",
    "Millis",
    "nextjs",
    "Nicifiable",
    "nicify",
    "openapi",
    "Proxied",
    "stackframe",
    "typehack",
    "Uncapitalize",
    "wolfgunblood"
  ],
  "[typescript]": {
      "editor.codeActionsOnSave": {
          "source.organizeImports": "explicit"
      }
  }
}
