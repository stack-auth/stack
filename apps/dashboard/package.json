    "lucide-react": "^0.378.0",
    "next": "^14.1",
    "next-runtime-env": "^3.2.2",
    "next-themes": "^0.2.1",
    "nodemailer": "^6.9.10",
    "openid-client": "^5.6.4",
    "posthog-js": "^1.149.1",
