    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.17",
    "glob": "^10.4.1",
    "postcss": "^8.4.38",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.2"
  }
